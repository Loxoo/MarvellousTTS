{
   "background": {
      "page": "BlindLens.html"
   },
   "browser_action": {
      "default_icon": "small_icon.png",
      "default_popup": "popup.html",
      "default_title": "Application pour les malvoyants, non voyants utilisant plusieurs API"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/addSpeech.js" ],
      "matches": [ "\u003Call_urls>", "https://*/*" ],
      "run_at": "document_end"
   } ],

   "description": "Application pour les malvoyants, non voyants utilisant plusieurs API",
   "icons": {
      "128": "images/icon.png",
      "16": "small_icon.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "14",
   "name": "BlindLens",
   "options_page": "options.html",
   "permissions": [ "\u003Call_urls>", "contextMenus", "ttsEngine", "tts", "tabs", "webRequest", "webNavigation", "unlimitedStorage", "webRequestBlocking", "https://*/*" ],
   "version": "1.0"
}
