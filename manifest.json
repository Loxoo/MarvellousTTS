{
   "background": {
      "scripts": [ "js/common.js", "js/background.js" ]
   },
   "browser_action": {
      "default_icon": "small_icon.png",
      "default_title": "Application pour les malvoyants, non voyants utilisant plusieurs API"  ,
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "css/highcontrast.css" ],
      "js": [ "js/highcontrast.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   } ],
   "description": "Application pour les malvoyants, non voyants utilisant plusieurs API",
   "icons": {
      "128": "images/icon.png",
      "16": "small_icon.png"
   },
   "default_locale": "fr",
   "manifest_version": 2,
   "name": "BlindLens",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "permissions": [ "\u003Call_urls>", "contextMenus", "ttsEngine", "tts", "tabs", "webRequest", "webNavigation", "unlimitedStorage", "webRequestBlocking", "https://*/*" ],
   "version": "1.0"
}
